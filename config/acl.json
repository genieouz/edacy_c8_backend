[
  {
    "group": "admin",
    "permissions": [
      {
        "resource": "*",
        "methods": "*",
        "action": "allow"
      }
    ]
  },
  {
    "group": "user",
    "permissions": [
      {
        "resource": "tache/",
        "methods": ["POST", "PUT"],
        "action": "deny"
      },
      {
        "resource": "tache/*",
        "methods": ["GET"],
        "action": "allow"
      }
    ]
  }
]
